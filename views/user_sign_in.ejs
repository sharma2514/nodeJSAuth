<h1>Sign in</h1>

<form action="/users/create-session" method="POST" id="sign_in_form">
  <input id="email" type="email" name="email" placeholder="Email" required />
  <input
    id="password"
    type="password"
    name="password"
    placeholder="password"
    required
  />

  <input type="submit" value="Sign In" />
</form>

<p>
  <a href="/users/auth/google">Google Sign In</a>
</p>
